<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1080.0" style="-fx-background-color: #FFD700;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AfterLogin">
   <children>
      <VBox alignment="CENTER" prefHeight="185.0" prefWidth="1080.0">
         <children>
            <Label text="Schedule Sketcher">
               <font>
                  <Font size="26.0" />
               </font>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </Label>
            <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="38.0" prefWidth="95.0" text="Time Zone: ">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <padding>
                        <Insets top="15.0" />
                     </padding>
                  </Label>
                  <Label fx:id="timezoneID" text="Label">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="169.0" prefWidth="1080.0" spacing="30.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="schedulebutton" layoutX="532.0" layoutY="91.0" mnemonicParsing="false" onAction="#hitScheduleButton" text="Schedule Appointment">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="modifyAppointment" layoutX="577.0" layoutY="91.0" mnemonicParsing="false" onAction="#hitModifyAppointment" text="Modify Appointment">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="deleteButton" layoutX="607.0" layoutY="121.0" mnemonicParsing="false" onAction="#hitDeleteButton" text="Delete Appointment">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="viewappointment" layoutX="661.0" layoutY="91.0" mnemonicParsing="false" onAction="#hitViewAppointment" text="View Booked Apts">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox layoutY="199.0" prefHeight="498.0" prefWidth="1080.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" />
            <HBox alignment="BOTTOM_RIGHT" prefHeight="41.0" prefWidth="1060.0">
               <children>
                  <Button fx:id="logout" mnemonicParsing="false" onAction="#userLogOut" prefHeight="44.0" prefWidth="80.0" text="Log out">
                     <font>
                        <Font size="13.0" />
                     </font>
                     <HBox.margin>
                        <Insets bottom="15.0" right="15.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
